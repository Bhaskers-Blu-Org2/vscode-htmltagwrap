{
	"name": "htmltagwrap",
	"description": "Wraps selected code with HTML tags",
	"version": "0.0.1",
	"publisher": "bgashler1",
	"engines": {
		"vscode": "*"
	},
	"activationEvents": [
		"command:extension.sayHello"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [{
			"command": "extension.sayHello",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "*"
	}
}